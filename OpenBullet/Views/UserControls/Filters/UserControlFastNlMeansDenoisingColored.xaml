<UserControl
    x:Class="OpenBullet.Views.UserControls.Filters.UserControlFastNlMeansDenoisingColored"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:OpenBullet.Views.UserControls.Filters"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <Label Content="Strength:" />
        <TextBox
            Name="StrengthTextBox"
            TextChanged="TextBox_TextChanged" />
        <Label Content="Strength(but for color images only):" />
        <TextBox
            Name="ColorStrengthTextBox"
            TextChanged="TextBox_TextChanged" />
    </StackPanel>
</UserControl>