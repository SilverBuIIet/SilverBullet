        Color="{StaticResource Color_009}" />
    <SolidColorBrush
        x:Key="AccordionButtonBackgroundBrush"
        Color="{StaticResource Color_009}" />
    <SolidColorBrush
        x:Key="AccordionButtonBorderBrush"
