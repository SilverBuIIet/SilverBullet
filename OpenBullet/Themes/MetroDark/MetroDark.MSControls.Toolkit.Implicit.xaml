    <SolidColorBrush
        x:Key="RatingItemHoverBackgroundBrush"
        Color="{StaticResource Color_023}" />
    <SolidColorBrush
        x:Key="RatingItemHoverBorderBrush"
        Color="{StaticResource Color_023}" />
    <SolidColorBrush
